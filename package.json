    "react-helmet-async": "^1.2.3",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.2",
    "react-ssr-prepass": "^1.5.0",
    "serialize-javascript": "^6.0.0",
    "serve-favicon": "^2.5.0",
    "serve-static": "^1.14.2",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@loadable/webpack-plugin": "^5.15.2",
