    "react-helmet-async": "^1.0.9",
    "react-redux": "^7.2.5",
    "react-ssr-prepass": "^1.4.0",
    "serialize-javascript": "^6.0.0",
    "serve-favicon": "^2.5.0",
    "serve-static": "^1.14.1"
  },
